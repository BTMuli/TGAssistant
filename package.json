{
  "name": "tg-assistant",
  "version": "1.4.0",
  "description": "The assistant for TeyvatGuide",
  "type": "module",
  "packageManager": "pnpm@8.10.5",
  "scripts": {
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write .",
    "test:all": "mocha --timeout 10000",
    "resize": "bun ./core/utils/resizeImages.ts",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,json,yml}": "eslint --fix",
    "*.{md,json,yml,ts}": "prettier --write"
  },
  "keywords": [
    "BTMuli",
    "Genshin",
    "Tools"
  ],
  "author": "BTMuli <bt-muli@outlook.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/BTMuli/TGAssistant.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/BTMuli/TGAssistent#readme",
  "bugs": {
    "url": "https://github.com/BTMuli/TGAssistent/issues"
  },
  "dependencies": {
    "app-root-path": "^3.1.0",
    "axios": "^1.6.2",
    "better-sqlite3": "^9.2.2",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0-rc.12",
    "fs-extra": "^11.2.0",
    "log4js": "^6.9.1",
    "sharp": "^0.33.0",
    "uuid": "^9.0.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/better-sqlite3": "^7.6.8",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.4",
    "@types/uuid": "^9.0.7",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsonc": "^2.10.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-yml": "^1.10.0",
    "husky": "^8.0.3",
    "jsonc-eslint-parser": "^2.4.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.1",
    "yaml-eslint-parser": "^1.2.2"
  }
}
