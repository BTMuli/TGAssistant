{
  "name": "tg-assistant",
  "version": "1.3.0",
  "description": "The assistant for Tauri.Genshin",
  "type": "module",
  "packageManager": "pnpm@8.7.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/BTMuli/TGAssistant.git"
  },
  "keywords": [
    "BTMuli",
    "Genshin",
    "Tools"
  ],
  "scripts": {
    "lint:all": "pnpm lint:style && pnpm run lint:fix",
    "lint:style": "prettier --write .",
    "lint:fix": "eslint --ext .js . --fix",
    "test:all": "mocha --timeout 10000",
    "resize": "node ./src/tools/resize.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.{js,css,md,json,yml}": "prettier --write"
  },
  "author": "BTMuli <bt-muli@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BTMuli/TGAssistent/issues"
  },
  "homepage": "https://github.com/BTMuli/TGAssistent#readme",
  "dependencies": {
    "app-root-path": "^3.1.0",
    "axios": "^1.5.0",
    "cheerio": "^1.0.0-rc.12",
    "log4js": "^6.9.1",
    "sharp": "^0.32.5"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "mocha": "^10.2.0",
    "prettier": "^3.0.3"
  }
}
